
/* does not work when result is float */
divideF(float x, float y, pointer float result){
    result = x/y;
}

/* does not work when result is float */
divide(int x, int y, pointer int result){
    result = x/y;
}

/* main */
main{

    /* declare int - display error when float value is assigned to int type
       Incorrect datatype for assignment
    */
    int i1; i1 = 45; print i1;  
    float a; a = 23.00;
    float b; b = 7.5;
    float result; result = 0.0;
    eval divideF(a, b, result);
    print(result);  
}