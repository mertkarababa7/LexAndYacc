/* main */
main{
    /* if-else example 1 when if is true */
    
    int i1; i1 = 0;
    if(i1 == 0){
        print(i1);
    } else {
        print(10000);
    }
    
    /* if-else example 1 when if is false  */
     
     int i2; i2 = 1;
     if(i2 == 0){
        print(i2);
    } else {
        print(10000);
    }
    
    }
    
