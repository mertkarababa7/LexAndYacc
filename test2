/* print function */
printFn(int x){
    print x;
}

add(int x, int y, pointer int result){
    result = x+y;
}

/* does not work when result is float */
divide(int x, int y, pointer int result){
    result = x/y;
}

/* does not work when result is float */
multiply(int x, int y, pointer int result){
    result = x*y;
}

/* main */
main{

    /* declare float */
    float f1; f1 = 6.459; print f1;

    /* declare int */
    int i1; i1 = 45; print i1;
    int i2; i2 = 123; print i2;
    int i3; i3 = i1 + i2;
    print i3;

    /* declare boolean */
    bool b1; b1 = TRUE; print b1;
    bool b2; b2 = FALSE; print b2;

    
    int i; i = 9;
    print(i+900);
    int j; j = 4;

    /* for */
    for (j = 0 to 5){
        print j;
    }

    /* while */ 
    int k; k = 12;   
    while(k >= 0){
        print k;
        k = k - 1;
    }

    /* if-else */
    int i4; i4 = 0;
    if(i4 == 0){
        print(i4 + 1000019);
    } else {
        print(10000);
    }

    /* function call */
    int x; x = 71;
    eval printFn(x);

    int a; a = 40;
    int b; b = 20;
    int iResult; iResult = 0;
    eval add(a, b, iResult);
    print(iResult);

    eval divide(a, b, iResult);
    print(iResult);

    eval multiply(a, b, iResult);
    print(iResult);
}